{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build LambdaPipelines GUI",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/LambdaPipelines.Gui/LambdaPipelines.Gui.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build Example Console App",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/LambdaPipelines.ExampleConsoleApp/LambdaPipelines.ExampleConsoleApp.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build Tests",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/LambdaPipelines.Triggers.Tests/LambdaPipelines.Triggers.Tests.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}
